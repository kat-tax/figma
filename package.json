{
  "version": "0.46.0",
  "name": "@kat.tax/figma-to-react-native",
  "description": "Transform Figma to React Native",
  "author": "ΚΛΤΤΛΧ",
  "license": "BSL",
  "repository": {
    "type": "git",
    "url": "https://github.com/kat-tax/figma-to-react-native"
  },
  "scripts": {
    "figma": "pnpm --filter ./plugins/figma run dev",
    "vscode": "pnpm --filter ./plugins/vscode run dev",
    "build:figma": "pnpm --filter ./plugins/figma run build",
    "build:vscode": "pnpm --filter ./plugins/vscode run build",
    "gen:figma-ui": "pnpm --filter ./packages/figma-ui run build",
    "gen:figma-build": "pnpm --filter ./packages/figma-build run build",
    "gen:figma-schema": "pnpm --filter ./plugins/figma run schema",
    "postinstall": "pnpm gen:figma-build && pnpm gen:figma-ui && pnpm gen:figma-schema && pnpm build:figma && pnpm build:vscode"
  }
}
