{
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "dev": "build-figma-plugin --typecheck --watch",
    "build": "build-figma-plugin --typecheck --minify",
    "schema": "typescript-json-schema tsconfig.json Settings --out ./src/schemas/settings.json"
  },
  "figma-plugin": {
    "id": "821138713091291738",
    "name": "Figma -> React Native",
    "networkAccess": {
      "allowedDomains": [
        "*"
      ],
      "reasoning": "Needed for Monaco Editor blob workers to load"
    },
    "permissions": [
      "currentuser"
    ],
    "editorType": [
      "figma",
      "dev"
    ],
    "capabilities": [
      "codegen",
      "inspect"
    ],
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "^2.6.1",
    "@create-figma-plugin/utilities": "^2.6.1",
    "@logtail/browser": "^0.4.18",
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@uppy/core": "^3.7.1",
    "@uppy/tus": "^3.4.0",
    "blakejs": "^1.2.1",
    "client-zip": "^2.4.4",
    "code-block-writer": "^12.0.0",
    "constrained-editor-plugin": "^1.3.0",
    "deep-object-diff": "^1.1.9",
    "esbuild-wasm": "^0.17.19",
    "fzf": "^0.5.2",
    "lodash.camelcase": "^4.3.0",
    "monaco-editor": "^0.44.0",
    "monaco-editor-auto-typings": "^0.4.5",
    "preact": "^10.18.1",
    "react-photo-album": "^2.3.0",
    "reserved": "^0.1.2",
    "y-monaco": "^0.1.5",
    "y-webrtc": "^10.2.6",
    "yjs": "^13.6.10"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^2.6.1",
    "@create-figma-plugin/tsconfig": "^2.6.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@figma/plugin-typings": "1.79.0",
    "@types/lodash.camelcase": "^4.3.9",
    "@types/react": "^18.2.41",
    "esbuild": "^0.17.19",
    "react-native-unistyles": "^1.2.0",
    "react-zoom-pan-pinch": "^3.3.0",
    "typescript": "^4.9.5",
    "typescript-json-schema": "^0.62.0"
  },
  "browser": {
    "crypto": false
  }
}
