{
  "name": "@kat-tax/figma-plugin",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "dev": "build-figma-plugin --typecheck --watch",
    "build": "build-figma-plugin --typecheck --minify",
    "schema": "pnpm schema-user-gen && pnpm schema-user-val && pnpm schema-project-gen && pnpm schema-project-val",
    "schema-user-gen": "typescript-json-schema tsconfig.json UserSettings --out ./src/interface/schemas/user-settings.json",
    "schema-user-val": "ajv compile -s ./src/interface/schemas/user-settings -o ./src/interface/schemas/user-settings.validator.js",
    "schema-project-gen": "typescript-json-schema tsconfig.json ProjectSettings --out ./src/interface/schemas/project-settings.json",
    "schema-project-val": "ajv compile -s ./src/interface/schemas/project-settings -o ./src/interface/schemas/project-settings.validator.js"
  },
  "figma-plugin": {
    "id": "821138713091291738",
    "name": "Figma -> React Native",
    "networkAccess": {
      "allowedDomains": [
        "*"
      ],
      "reasoning": "Needed for Monaco Editor blob workers to load"
    },
    "permissions": [
      "currentuser"
    ],
    "editorType": [
      "figma",
      "dev"
    ],
    "capabilities": [
      "codegen",
      "inspect",
      "vscode"
    ],
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  },
  "dependencies": {
    "@blocknote/core": "^0.10.1",
    "@blocknote/react": "^0.10.1",
    "@create-figma-plugin/utilities": "^3.1.0",
    "@hocuspocus/provider": "^2.8.1",
    "@iconify/react": "^4.1.1",
    "@logtail/browser": "^0.4.18",
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@uidotdev/usehooks": "^2.4.1",
    "@uppy/core": "^3.8.0",
    "@uppy/tus": "^3.5.0",
    "@y-sweet/client": "^0.1.0",
    "@y-sweet/react": "^0.1.0",
    "@zip.js/zip.js": "^2.7.32",
    "blakejs": "^1.2.1",
    "code-block-writer": "^12.0.0",
    "constrained-editor-plugin": "^1.3.0",
    "css-to-rn": "workspace:*",
    "deep-object-diff": "^1.1.9",
    "figma-ui": "workspace:*",
    "fzf": "^0.5.2",
    "lib0": "^0.2.88",
    "lodash.camelcase": "^4.3.0",
    "monaco-editor": "^0.45.0",
    "monaco-editor-auto-typings": "^0.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-photo-album": "^2.3.0",
    "react-virtuoso": "^4.6.2",
    "reserved": "^0.1.2",
    "uniforms": "4.0.0-alpha.5",
    "uniforms-bridge-json-schema": "4.0.0-alpha.5",
    "uniforms-semantic": "4.0.0-alpha.5",
    "web-bundler": "workspace:*",
    "y-protocols": "^1.0.6",
    "y-webrtc": "^10.3.0",
    "yjs": "^13.6.10"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^3.1.0",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@figma/plugin-typings": "1.82.0",
    "@types/lodash.camelcase": "^4.3.9",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "ajv-cli": "^5.0.0",
    "esbuild": "^0.19.11",
    "react-zoom-pan-pinch": "^3.3.0",
    "typescript": "^5.3.3",
    "typescript-json-schema": "^0.62.0"
  },
  "browser": {
    "crypto": false
  }
}
