{
  "extends": "@create-figma-plugin/tsconfig",
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/vendor/css-to-rn/index.js",
    "src/vendor/css-to-rn/devPropertiesWithoutUnitsRegExp.js"
  ],
  "compilerOptions": {
    "target": "ES2020",
    "baseUrl": "./src",
    "typeRoots": [
      "node_modules/@figma",
      "node_modules/@types"
    ],
    "strict": false,
    "skipLibCheck": true,
  }
}
