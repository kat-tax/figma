{
  "name": "figma-plugin",
  "private": true,
  "license": "GPL-3.0",
  "figma-plugin": {
    "id": "821138713091291738",
    "name": "Figma -> React Native",
    "networkAccess": {
      "allowedDomains": [
        "*"
      ],
      "reasoning": "Needed for Monaco Editor blob workers to load"
    },
    "permissions": [
      "currentuser"
    ],
    "editorType": [
      "figma",
      "dev"
    ],
    "capabilities": [
      "codegen",
      "inspect",
      "panel"
    ],
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  },
  "scripts": {
    "dev": "bun run scripts/index.ts --dev",
    "gen": "bun run scripts/index.ts",
    "schema-settings": "typescript-json-schema tsconfig.json Settings --include ./src/types/settings.ts --out ./src/schemas/settings.json"
  },
  "dependencies": {
    "@hocuspocus/provider": "^2.8.0",
    "@logtail/browser": "^0.4.16",
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@uppy/core": "^3.7.1",
    "@uppy/tus": "^3.4.0",
    "client-zip": "^2.4.4",
    "code-block-writer": "^12.0.0",
    "constrained-editor-plugin": "^1.3.0",
    "deep-object-diff": "^1.1.9",
    "esbuild-wasm": "^0.17.19",
    "figma-ui": "workspace:*",
    "fzf": "^0.5.2",
    "lodash.camelcase": "^4.3.0",
    "monaco-editor": "^0.44.0",
    "monaco-editor-auto-typings": "^0.4.4",
    "preact": "^10.19.2",
    "react-photo-album": "^2.3.0",
    "reserved": "^0.1.2",
    "y-monaco": "^0.1.5",
    "y-webrtc": "^10.2.5",
    "yjs": "^13.6.8"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^2.6.1",
    "@create-figma-plugin/tsconfig": "^2.6.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@figma/plugin-typings": "^1.81.0",
    "@types/lodash.camelcase": "^4.3.9",
    "@types/react": "^18.2.37",
    "typescript-json-schema": "^0.62.0",
    "esbuild": "^0.19.7",
    "esbuild-css-modules-plugin": "3.0.3",
    "find-up": "7.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
